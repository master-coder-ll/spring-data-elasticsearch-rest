{
    "settings" : {
        "number_of_shards" : 1,
        "refresh_interval" : "1s",
        "number_of_replicas" : 1,
        "store.type": "fs",
        "index": {
            "blocks": {
                "read_only_allow_delete": "false"
            }
        }
    },
    "aliases": {
        "search-vehicles-alias" : {},
        "write-vehicles-alias" : {}
    },
    "mappings": {
      "_doc": {
        "properties": {
          "vehicleId": {
            "type": "keyword"
          },
          "location": {
            "type": "geo_point"
          },
          "time": {
            "type": "date"
          }
        }
      }
    }
}
